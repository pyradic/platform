{
    "name": "pyro/platform",
    "require": {
        "pyro/webpack": "~1.0",
        "inertiajs/inertia-laravel": "0.2.*",
        "livewire/livewire": "0.5.*",
        "laradic/support": "~2.0",
        "laradic/generators": "~2.0",
        "laradic/dependency-sorter": "~1.0",
        "laradic/config": "~2.0",
        "eddiriarte/console-select": "~2.0",
        "jackiedo/dotenv-editor": "^1.0",
        "symfony/serializer": "~4.0",
        "symfony/property-access": "~4.0",
        "tightenco/ziggy": "~0.8",
        "fico7489/laravel-pivot": "~3.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Pyro\\Platform\\": "src",
            "Anomaly\\Streams\\Platform\\": "streams"
        },
        "exclude-from-classmap": [
            "vendor/anomaly/streams-platform/src/Ui/Table/Component/Action/ActionBuilder.php",
            "vendor/anomaly/streams-platform/src/Ui/ControlPanel/Component/Button/ButtonBuilder.php",
            "vendor/anomaly/streams-platform/src/Ui/ControlPanel/Component/Button/ButtonGuesser.php",
            "vendor/anomaly/streams-platform/src/Ui/Icon/IconRegistry.php",
            "vendor/anomaly/streams-platform/src/Traits/FiresCallbacks.php",
            "vendor/anomaly/streams-platform/src/Database/Seeder/Seeder.php",
            "vendor/anomaly/streams-platform/src/Support/Hydrator.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
