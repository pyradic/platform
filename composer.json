{
    "name": "pyro/platform",
    "require": {
        "eddiriarte/console-select": "~2.0",
        "laradic/support": "~2.0",
        "laradic/generators": "~2.0",
        "laradic/dependency-sorter": "~1.0",
        "laradic/config": "~2.0",
        "jackiedo/dotenv-editor": "^1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Pyro\\Platform\\": "src",
            "Anomaly\\Streams\\Platform\\": "streams"
        },
        "exclude-from-classmap": [
            "vendor/anomaly/streams-platform/src/Ui/Table/Component/Action/ActionBuilder.php",
            "vendor/anomaly/streams-platform/src/Ui/ControlPanel/Component/Button/ButtonBuilder.php",
            "vendor/anomaly/streams-platform/src/Ui/ControlPanel/Component/Button/ButtonGuesser.php",
            "vendor/anomaly/streams-platform/src/Traits/FiresCallbacks.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
